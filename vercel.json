{
    "version": 2,
    "builds": [
      {
        "src": "/api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/ministros",
        "dest": "/api/ministros.js"
      },
      {
        "src": "/api/musicas",
        "dest": "/api/musicas.js"
      },
      {
        "src": "/api/escalas",
        "dest": "/api/escalas.js"
      },
      {
        "src": "/api/repertorios",
        "dest": "/api/repertorios.js"
      }
    ]
  }
  